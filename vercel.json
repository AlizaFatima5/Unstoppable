{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "BookYourBarber/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "BookYourBarber.settings"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.10"
    },
    {
    "src":"BookyourBarber/wsgi.py",
    "use":"@vercel/python"
    }
  }
}
